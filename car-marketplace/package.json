{
  "name": "carmarket-bg",
  "version": "1.0.0",
  "description": "Bulgarian Car Marketplace Platform with AI Features",
  "type": "module",
  "author": "CarMarketBG Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/carmarket-bg.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/carmarket-bg/issues"
  },
  "homepage": "https://carmarketbg.com",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "analyze": "vite-bundle-analyzer",
    "firebase:deploy": "firebase deploy",
    "firebase:serve": "firebase serve",
    "firebase:emulators": "firebase emulators:start",
    "pwa:validate": "workbox-cli validate",
    "ai:train": "node scripts/ai-training.js",
    "data:collect": "node scripts/data-collection.js",
    "db:migrate": "node scripts/db-migrate.js"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
