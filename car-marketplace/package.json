    "@vueuse/head": "^2.0.0",
    "firebase": "^10.7.1",
    "axios": "^1.6.5",
    "date-fns": "^3.2.0",
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.0.18",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "chart.js": "^4.4.1",
    "vue-chartjs": "^5.3.0",
    "mapbox-gl": "^3.1.0",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "intersection-observer": "^0.12.2",
    "fuse.js": "^7.0.0",
    "dompurify": "^3.0.7",
    "marked": "^11.1.1",
    "qrcode": "^1.5.3",
    "jszip": "^3.10.1",
    "file-saver": "^2.0.5",
