  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "firebase",
    "pwa",
    "car-marketplace",
    "bulgaria",
    "ai",
    "chatbot",
    "image-analysis"
  ]
}
